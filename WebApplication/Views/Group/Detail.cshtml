@model IEnumerable<WebApplication.Models.User>

@{
	ViewBag.Title = "Detail";
	Layout = "~/Views/Shared/_LayoutTable.cshtml";
	int i = 1;
}


<div class="col-md-12 col-sm-12 col-xs-12">
	<div class="x_panel">
		<div class="x_title">
			<div class="row">
				<h2>Group Detail</h2>
			</div>
			<div class="row">
				<input type="button" value="Back" style="margin-left: 15px;" class="btn btn-default" onclick="window.location='/Group';" />
				<a href="~/Group/Import/@Session["GroupID"]">
					<button type="button" class="btn btn-default">
						Import
					</button>
				</a>
			</div>


			<div class="clearfix"></div>
		</div>
		<div class="x_content">
			<table id="datatable-buttons" class="table table-striped table-bordered">
				<thead>
					<tr>
						<th>No.</th>
						<th>StID</th>
						<th>Full Name</th>
						@*<th>Last Name</th>*@
						<th>Email</th>
						<th>DoB</th>
						<th>Note</th>
					</tr>
				</thead>

				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<td>@(i++)</td>
							<td>
								@Html.DisplayFor(modelItem => item.StID)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.FullName)
							</td>
							@*<td>
								@Html.DisplayFor(modelItem => item.PhoneNumber)
							</td>*@
							<td>
								@Html.DisplayFor(modelItem => item.Email)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.DoB)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.Note)
							</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>

	</div>
